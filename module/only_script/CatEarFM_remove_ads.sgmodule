[Script]
# 首页顶部入口
精简首页顶部入口 = type=http-response, pattern=^https:\/\/app\.missevan\.com\/site\/icons\?, script-path=https://kelee.one/Resource/Script/CatEarFM/CatEarFM_remove_ads.js, requires-body=true, script-update-interval=-1

# 发现页面 - 直播间、广播剧、免流服务
精简发现页面 = type=http-response, pattern=^https:\/\/app\.missevan\.com\/discovery\/list$, script-path=https://kelee.one/Resource/Script/CatEarFM/CatEarFM_remove_ads.js, requires-body=true, script-update-interval=-1

[MITM]
hostname = %APPEND% app.missevan.com, fm.missevan.com

