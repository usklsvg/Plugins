http:

  mitm:
    - "confssl*.iweimai.cn"

  body-rewrite:
    - >-
      ^https:\/\/confssl\d?\.iweimai\.cn\/wm_ios_update\.json\? response-jq if (getpath([]) | has("isShowCJAd")) then (setpath(["isShowCJAd"]; 0)) else . end
    - >-
      ^https:\/\/confssl\d?\.iweimai\.cn\/wm_ios_update\.json\? response-jq delpaths([["launchAd"]])
    - >-
      ^https:\/\/confssl\d?\.iweimai\.cn\/wm_ios_update\.json\? response-jq delpaths([["adBanners"]])
    - >-
      ^https:\/\/confssl\d?\.iweimai\.cn\/wm_ios_update\.json\? response-jq delpaths([["ad"]])

